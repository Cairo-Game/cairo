{
  "name": "Cairo-Game",
  "version": "1.0.0",
  "description": "Game realized with React js",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development npm-run-all --print-label --parallel start:*",
    "build": "webpack --mode production && ts-node index.ts",
    "test": "jest --env=jsdom",
    "lint": "eslint --fix . --ext .js,.jsx,.ts,.tsx --ignore-pattern node_modules/",
    "prettier": "prettier --write  ./src/**/**/*.ts[x]",
    "start:dev": "NODE_PATH=./dist CFG_DIR=$(pwd)/dist/configs node dist/server/index.js",
    "build:configs": "tsc --project configs",
    "build:server": "tsc --project server",
    "build:dev:vendors": "NODE_ENV=development webpack --mode=development --config webpack/configs/vendors.config.ts",
    "build:ssr": "webpack --mode=production --hide-modules --config webpack/config/ssr.config.ts",
    "build:client": "better-npm-run build:client",
    "init": "yarn install && npm install && npm run build:dev:vendors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cairo-Game/frontend.git"
  },
  "license": "ISC",
  "author": "Loboda Igor",
  "bugs": {
    "url": "https://github.com/Cairo-Game/frontend/issues"
  },
  "homepage": "https://github.com/Cairo-Game/frontend#readme",
